{
  "name": "scikit",
  "version": "1.0.0",
  "description": "Scikit-Learn for JS",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "npm run clean && npm run build:only && npm run test:src && npm run prettier && npm run delete:shared",
    "test:src": "nyc mocha --require ts-node/register 'src/**/*.test.ts'",
    "test:clean": "npm run test:node && npm run test:browser",
    "test:browser": "cd scikitjs-browser && npm run test",
    "test:node": "cd scikitjs-node && npm run test",
    "build:browser": "cd scikitjs-browser && npm run build",
    "build:node": "cd scikitjs-node && npm run build",
    "build": "npm run build:node && npm run build:browser",
    "build:docs": "cd docs && npm run build",
    "postinstall": "(cd docs && npm install); (cd scikitjs-browser && npm install); (cd scikitjs-node && npm install); (cd shared && npm install); (npx husky install);"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/javascriptdata/scikit.js.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/javascriptdata/scikit.js/issues"
  },
  "homepage": "https://github.com/javascriptdata/scikit.js#readme",
  "dependencies": {
    "danfojs": "^0.3.4",
    "danfojs-node": "^0.3.4",
    "eslint": "^8.2.0",
    "lodash": "^4.17.21",
    "mathjs": "^10.0.0",
    "seedrandom": "^3.0.5",
    "simple-statistics": "^7.7.0",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@tensorflow/tfjs": "^3.11.0",
    "@tensorflow/tfjs-node": "^3.11.0",
    "@types/chai": "^4.2.22",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@types/lodash": "^4.14.177",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.1",
    "@types/node": "^16.9.6",
    "@types/table": "^6.3.2",


    "coveralls": "^3.1.0",
    "jsdoc": "^3.6.7",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.59.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-node": "^10.0.0",
    "tslib": "*",
    "eslint": "^8.4.1",
    "husky": "^7.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "public",
    "branches": [
      "main"
    ]
  },
  "keywords": [
    "pandas",
    "data-analysis",
    "data-manipulation",
    "analysis"
  ],
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
